{
  "manifest_version": 3,
  "name": "Google Search AI block",
  "version": "1.0",
  "description": "removes AI images from google image search results",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.google.com/search*"],
      "js": ["block.js"]
    }
  ]
}
